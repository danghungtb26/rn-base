{
  "extends": [
    "satya164"
  ],
  "rules": {
    "babel/no-invalid-this": 0,
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "semi": false
      }
    ],
    "no-console":"warn",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off",
    
    "@typescript-eslint/member-delimiter-style": ["error", {
      "multiline": {
        "delimiter": "none",
        "requireLast": true
      },
      "singleline": {
          // "delimiter": "none",
          "requireLast": false
      },
      "overrides": {
          "interface": {
              "multiline": {
                  "delimiter": "none",
                  "requireLast": true
              }
          }
      }
    }]

  },
  "settings": {
    "import/core-modules": [
        "@dvh/core",
        "@dvh/splash",
        "@dvh/native-common",
        "@dvh/element"
      ]
  }
}