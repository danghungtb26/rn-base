{
  "name": "@dvh-module/core",
  "version": "0.1.1",
  "description": "core",
  "main": "lib/commonjs/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__"
  ],
  "scripts": {
    "test": "jest",
    "prepare:tsc": "bob build",
    "prepare": "yarn clean && tsc --composite false -p tsconfig.build.json",
    "clean": "rm -rf lib */index.{js,js.map,d.ts} index.{js,js.map,d.ts}"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danghungtb26/dvh-module.git",
    "directory": "packages/core"
  },
  "author": "Đặng Hùng <danghungtb26@gmail.com> (https://github.com/danghungtb26)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danghungtb26/dvh-module-core/issues"
  },
  "homepage": "https://github.com/danghungtb26/dvh-module-core#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.167",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "axios": "^0.21.1",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "lodash": "^4.17.20",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react-native-builder-bob": "^0.17.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "axios": "*",
    "lodash": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "gitHead": "4bcdfe8a9fbb3c9465bd14dee218c26f1f883312"
}
